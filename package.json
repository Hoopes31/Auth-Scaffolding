{
  "name": "auth-scaffolding",
  "version": "1.0.0",
  "description": "base project for client/server",
  "main": "server/src/index.js",
  "scripts": {
    "dev": "concurrently \"npm run startclient \" \"nodemon server/src/index.js \" ",
    "startserver": "node server/src/index.js",
    "testserver": "mocha -b --reporter=min --timeout 10000",
    "pretty": "prettier --write './src/**/*.js'",
    "startclient": "node client/scripts/start.js",
    "build:client": "node client/scripts/build.js",
    "testclient": "jest",
    "start": "npm run build:client && npm run startserver --",
    "testprod": "nodemon server/src/index.js --production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hoopes31/Auth-Scaffolding.git"
  },
  "author": "Hoopes31 & nicholaswbowen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hoopes31/Auth-Scaffolding/issues"
  },
  "dependencies": {
    "autoprefixer": "7.1.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "bootstrap-sass": "^3.3.7",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "chalk": "^1.1.3",
    "colors": "^1.1.2",
    "concurrently": "^3.5.0",
    "css-loader": "0.28.1",
    "dotenv": "^4.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.0.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "express": "^4.15.3",
    "express-jwt": "^5.3.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "helmet": "^3.6.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.3",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "mongoose": "^4.10.5",
    "morgan": "^1.8.2",
    "node-emoji": "^1.5.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "prettier": "^1.4.4",
    "promise": "7.1.1",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.1.0",
    "react-bootstrap": "^0.31.0",
    "react-dev-utils": "^3.0.0",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.7",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.1",
    "react-scripts": "^1.0.10",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "^3.0.0",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:3001",
  "jest": {
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/client/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/client/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/client/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  }
}
